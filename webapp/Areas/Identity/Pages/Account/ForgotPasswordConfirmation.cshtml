@page "/forgot-password-confirmation"
@model ForgotPasswordConfirmation
@{
    ViewData["Title"] = "Forgot password confirmation";
}

<section>
    <div class="container">
		<h1>@ViewData["Title"]</h1>
		<p>
			Please check your email to reset your password. <a class="text-decoration-underline" asp-page="./Login">Return to login</a>
		</p>
    </div>
</section>

